{
    "name": "rarity-levelup",
    "version": "0.0.1",
    "description": "Quick app for automating rarity summoner adventuring and leveling up",
    "main": "main.js",
    "scripts": {
        "build": "docker build --tag rarity-bot .",
        "lint": "eslint -c .eslintrc.js . *.js",
        "run": "node src/main.js",
        "prettier": "prettier --write .",
        "mint": "node actions/mint.js",
        "prepare": "husky install"
    },
    "keywords": [
        "ethereum",
        "rarity",
        "nft"
    ],
    "author": "Antonio Savage",
    "license": "MIT",
    "dependencies": {
        "@ethersproject/experimental": "^5.4.0",
        "dotenv": "^10.0.0",
        "ethers": "^5.4.6"
    },
    "devDependencies": {
        "@typechain/ethers-v5": "^7.1.0",
        "@types/node": "^16.9.2",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.4.0",
        "ts-generator": "^0.1.1",
        "ts-node": "^10.2.1",
        "typechain": "^5.1.2",
        "typescript": "^4.4.3",
        "husky": "^7.0.0"
    }
}
